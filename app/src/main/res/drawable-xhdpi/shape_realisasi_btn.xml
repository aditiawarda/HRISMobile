<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#FFF5F1" />
            <stroke
                android:width="1dp"
                android:color="#E39177">
            </stroke>
            <corners android:radius="6dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFEDE8" />
            <stroke
                android:width="1dp"
                android:color="#DA795B">
            </stroke>
            <corners android:radius="6dp"/>
        </shape>
    </item>
</selector>