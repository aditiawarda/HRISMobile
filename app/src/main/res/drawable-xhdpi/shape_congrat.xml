<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="360"
                android:centerX="10%"
                android:startColor="#FFBF3E"
                android:endColor="#A6441F"
                android:type="linear" />
            <corners android:radius="12dp"/>
            <stroke
                android:width="0.3dp"
                android:color="#99676767"/>
        </shape>
    </item>
</selector>

