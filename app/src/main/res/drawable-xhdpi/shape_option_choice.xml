<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="true">
    <shape android:shape="rectangle">
        <solid
            android:color="#97B6B6B6">
        </solid>

        <corners
            android:radius="3dp">
        </corners>

        <stroke
            android:width="1dp"
            android:color="#149322">
        </stroke>
    </shape>
</item>
<item>
    <shape android:shape="rectangle">
        <solid
            android:color="#F2FFF2">
        </solid>

        <corners
            android:radius="3dp">
        </corners>

        <stroke
            android:width="1dp"
            android:color="#149322">
        </stroke>
    </shape>
</item>
</selector>